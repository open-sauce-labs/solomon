		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"js-base64": "^3.7.2",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"swr": "^1.3.0",
		"tsc-alias": "^1.7.0",
		"typescript": "^4.8.4"
	}
}
